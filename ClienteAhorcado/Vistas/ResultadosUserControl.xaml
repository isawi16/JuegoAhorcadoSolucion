<UserControl x:Class="ClienteAhorcado.Vistas.ResultadosUserControl"
    xmlns:lex="http://wpflocalizeextension.codeplex.com"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Width="300" Height="400">
    <StackPanel Margin="20" Background="#FF05321C">
        <Label Name="lblGanador" Content="{lex:Loc Resources.ganador}" FontWeight="Bold" FontFamily="Bradley Hand ITC" Foreground="#FFEADFDF" FontSize="24"/>
        <Label Name="lblPalabraCorrecta" Content="{lex:Loc Resources.palabra_correcta}" FontFamily="Bradley Hand ITC" Foreground="#FFF8F0F0"/>
        <Label Name="lblPuntajes" Content="{lex:Loc Resources.puntajes}" FontFamily="Bradley Hand ITC" Foreground="#FFF8F0F0" FontWeight="Bold"/>
        <Button Content="{lex:Loc Resources.volver_menu_principal}" Width="250" Margin="5"/>
    </StackPanel>
</UserControl>
